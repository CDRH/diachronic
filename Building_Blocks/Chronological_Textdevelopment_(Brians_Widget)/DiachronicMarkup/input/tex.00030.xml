   <text>
      <body>
         <!--<ge:metaMark function="sequence"><del rend="overstrike">314</del></ge:metaMark>-->

         <l>
            <seg xml:id="l1" ge:stage="#l1-s01">
               <damage agent="ink">
                  <supplied resp="#bb">I </supplied>
               </damage>
               <del rend="overstrike" ge:stage="#l1-s02">am</del>
               <!--<metamark ge:stage="#l1-s02" target="#a01">&#8248; </metamark>-->
               <del rend="overstrike" ge:stage="#l1-s03">
                  <add xml:id="a01" place="above" ge:stage="#l1-s02" rend="insertion">can be</add>
               </del>
               <add place="above" ge:stage="#l1-s06" rend="unmarked">I <unclear reason="illegible"
                     cert="medium">am </unclear> a </add>
               <del rend="overstrike" ge:stage="#l1-s6">
                  <add place="inline" ge:stage="#l1-s04" rend="unmarked">can</add>
               </del>
               <add place="above" ge:stage="#l1-s03" rend="unmarked">become </add>
               <del rend="overstrike" ge:stage="#l1-s06">a</del>
               <del rend="overstrike" ge:stage="#l1-s05">corpse,</del>
               <add place="above" ge:stage="#l1-s05" rend="unmarked">shroud; </add>
               <del rend="overstrike" ge:stage="#l1-s05">shrouded and buried, </del>
            </seg>
         </l>

         <l>
            <seg xml:id="l2" ge:stage="#l2-s01"> I <del rend="overstrike" ge:stage="#l2-s02"
                  >have</del> wrap<del rend="overstrike" ge:stage="#l2-s02">ped</del> a <del
                  rend="overstrike" ge:stage="#l2-s02">man's</del>
               <del rend="overstrike" ge:stage="#l2-s02">body</del>
               <add place="above" ge:stage="#l2-s02" rend="unmarked">body</add>, and <lb/>
               <subst ge:stage="#l2-s02"><del rend="overstrike">am</del><add rend="unmarked"
                     place="above">lie </add>
               </subst> in the coffin with <del rend="overstrike" ge:stage="#l2-s02">him,</del>
               <add place="above" ge:stage="#l2-s02" rend="unmarked">it, </add>
            </seg>
         </l>


         <!--<ge:metaMark function="grouping" spanTo="#an01">&#8267; </ge:metaMark>-->
         <l>
            <seg xml:id="l3" ge:stage="#l3-s01">It is dark there underground<subst
                  ge:stage="#l3-s02"><del rend="overwrite">;</del><add place="above" rend="unmarked"
                     >, </add></subst>&#8212; </seg>
         </l>
         <l>
            <seg xml:id="l4" ge:stage="#l4-s01">It is not evil or pain there&#8212;it is the <lb/>
               absence of all that is good.&#8212; </seg>
         </l>
         <anchor xml:id="an01"/>

         <l>
            <seg xml:id="l5a" ge:stage="#l5-s01">Now it seems to me that <del rend="overstrike"
                  ge:stage="#l5-s06">any</del>
               <add place="above" ge:stage="#l5-s06" rend="unmarked">every </add> thing <del
                  rend="overstrike" ge:stage="#l5-s03">out</del>
               <del rend="overstrike" ge:stage="#l5-s02">of</del>
            </seg>
            <lb/>
            <seg xml:id="l5b" ge:stage="#l5-s02">
               <del rend="overstrike" ge:stage="#l5-s03">from</del>
               <del rend="overstrike" ge:stage="#l5-s04"><add place="above" ge:stage="#l5-s03"
                     rend="unmarked">not</add>
               </del>
               <del rend="overstrike" ge:stage="#l5-s04">under</del>
               <add place="above" ge:stage="#l5-s04" xml:id="add02" rend="unmarked">above</add>
               <ge:metaMark function="insertion" target="#add02" ge:stage="#l5-s04">&#8248; </ge:metaMark>
               <add place="above" ge:stage="#l5-s05" rend="insertion">in </add> the <del
                  rend="overstrike" ge:stage="#l5-s05">ground</del>
               <ge:metaMark function="insertion" target="#add01" ge:stage="#l5-s05">&#8248;
                  </ge:metaMark><add place="above" ge:stage="#l5-s05" xml:id="add01"
                  rend="insertion">light above </add> must be happy<add place="inline"
                  ge:stage="#l5-s06" rend="unmarked">, </add>
               <del rend="overstrike" ge:stage="#l5-s06">enough,</del>&#8212; </seg>
         </l>

         <l>
            <seg xml:id="l6a" ge:stage="#l6-s01">Whoever is not in his coffin, and the<lb/> dark
               grave, let him <del rend="overstrike" ge:stage="#l6-s02">reg</del>
            </seg>
            <seg xml:id="l6b" ge:stage="#s6-s02">know<lb/> he has enough. </seg>
         </l>

         <l>
            <seg xml:id="l7" ge:stage="#l7-s01">The retrospective extasy is upon me <add
                  place="above" ge:stage="#l7-s04" rend="unmarked">now&#8212; </add>&#8212;<del
                  rend="overstrike" ge:stage="#l7-s02">I am</del>
               <add place="above" ge:stage="#l7-s02" rend="unmarked">my <del rend="overstrike"
                     ge:stage="#l7-s03">soul</del>
               </add>
               <add place="above" ge:stage="#l7-s03" rend="unmarked">spirit </add>
               <add place="inline" ge:stage="#l7-s02" rend="unmarked">burns </add> volcanic </seg>
         </l>
         <l>
            <seg xml:id="l8" ge:stage="#l8-s01">The earth recedes ashamed before my<lb/> prophetical
               crisis.&#8212; </seg>
         </l>

      </body>
   </text>
