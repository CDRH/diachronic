    <text>
        <body>
            <pb n="02"/>
            <sp>
                <speaker n="before_9010i"><abbr>H.</abbr></speaker>
                <l n="9010">Wie ſieht er aus ich dencke wild wie Cacus wohl</l>
            </sp>
            <sp>
                <speaker n="before_9010f"><abbr>Ph</abbr></speaker>
                <l n="9011">Es iſt ein muntrer
                    <note><abbr>pp</abbr></note><!-- todo Kodierung zum pp prüfen --></l>
                <l n="9022">regelhaft</l>
                <!-- Todo was bedeutet die Streichung? -->
                <l n="9023">Schaut ſie von auſſen Himmelan <add f:revType="soon">ſtrebt ſie
                        empor</add></l>
                <l n="9024">So ſtarr, ſo wohl in Fugen, glatt wie Stahl</l>
                <l n="9025">Kein Klettern hier, ſelbſt der Gedancke gleitet ab</l>
                <l n="9026">Und innen großer H<orig>o</orig>fe R<orig>a</orig>umlichkeiten rings</l>
                <l n="9027">Mit Baulichkeit umgeben aller Art <choice>
                        <abbr>u</abbr>
                        <expan>und</expan>
                    </choice> Zweck</l>
                <l n="9028"><del f:revType="instant">Das</del> Da ſeht ihr Säulen,
                    S<orig>a</orig>ulchen Bogen B<orig>o</orig>gelchen</l>
                <l n="9029">Und Galerien umzuſchaun heraus herein</l>
                <l n="9030" xml:id="l9030a">Und Wappen</l>
            </sp>
            <sp>
                <speaker n="before_9030m">Chor</speaker>
                <l n="9030" xml:id="l9030b">Was ſind Wapen</l>
            </sp>
            <sp>
                <speaker n="before_9030f"><abbr>Ph.</abbr></speaker>
                <l n="9030" xml:id="l9030c">Aja<del f:revType="instant" f:correction="true"
                    >ck</del>x führte ja</l>
                <join target="#l9030a #l9030b #l9030c" type="antilabe"/>
                <l n="9031 9037">Geſchlungene Schlang im Schilde <subst f:revType="instant"
                        ge:stage="#change_l9037-9038">
                        <del>ſolcherley Ge<del f:correction="true" f:revType="instant"
                            >f</del>bild</del>
                        <add>denckt ihrs noch</add>
                    </subst></l>
                <l n="9037 9038"><del f:revType="instant" ge:stage="#change_l9037-9038">Führt jeder
                        auch <subst ge:stage="#change_l9038">
                            <del>V</del>
                            <add>v</add>
                        </subst>on ſei<surplus>h</surplus>nen Urur Ahnen her.</del></l>
                <l n="9037">Und ſolch Gebild führt hier ein jeder Helden Sohn</l>
                <l n="9038">Von ſeinen Ururahnen her in Farben Glanz</l>
                <pb n="03"/>
                <l n="9042"><subst f:revType="soon">
                        <del>Umher nun</del>
                        <add>Dergleichen</add>
                    </subst> h<orig>a</orig>ngt in Sälen Reih an Reihe fort</l>
                <l n="9043">In <subst ge:stage="#change_l9043-9044">
                        <del>ſ</del>
                        <add>S</add>
                    </subst>älen <subst ge:stage="#change_l9043-9044">
                        <del>weiter</del>
                        <add>gränzen loſen</add>
                    </subst> als die Welt <subst ge:stage="#change_l9043-9044">
                        <del>ſie ſcheinen mir</del>
                        <add>ſo weit</add>
                    </subst></l>
                <l n="9044" xml:id="l_9044a"><subst ge:stage="#change_l9043-9044">
                        <del>unendlich. Möchtet da zu tanzen!</del>
                        <add>Da könnt ihr tanzen</add>
                    </subst></l>
            </sp>
            <sp>
                <speaker n="before_9044f"><subst>
                        <del><abbr>Chf</abbr></del>
                        <add><abbr>Ch</abbr></add>
                    </subst></speaker>
                <l n="9044" xml:id="l9044b"><add f:revType="soon">Sage</add>
                    <subst>
                        <del>G</del>
                        <add>g</add>
                    </subst>iebts auch Tänzer da</l>
                <join target="#l_9044a #l9044b" type="antilabe"/>
            </sp>
        </body>
    </text>
